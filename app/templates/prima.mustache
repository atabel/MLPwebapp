<div class="prima-wrap">
	<div class="prima-numeric layout">
		<div>
			<strong class="prima-value">{{prima_value}}</strong>
		</div>
		<div id="prima-numbers">
			<div class="layout horizontal">
				<div class="prima-delta {{^is_growing}}negative-val{{/is_growing}} {{#is_growing}}positive-val{{/is_growing}} ">
					{{#is_growing}}
					<span class="icon angle-up"></span>
					{{/is_growing}}
					{{^is_growing}}
					<span class="icon angle-down"></span>
					{{/is_growing}}
					{{prima_delta_abs}}
				</div>
				<div class="prima-percent {{^is_growing}}negative-val{{/is_growing}} {{#is_growing}}positive-val{{/is_growing}} ">
					{{#is_growing}}
					<span class="icon angle-up"></span>
					{{/is_growing}}
					{{^is_growing}}
					<span class="icon angle-down"></span>
					{{/is_growing}}
					{{prima_percent_abs}}
				</div>
			</div>
		</div>
	</div>
	<div class="prima-chart">
	</div>
</div>