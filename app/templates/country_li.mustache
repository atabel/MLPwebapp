<li class="country-li selectable" data-country-code="{{country_code}}">
	<div class="on-right bubble red li-prima-value" data-count="{{prima_value}}">
		<span class="tag count">{{prima_value}}</span>
	</div>
	<a href="#" data-view-section="section-prima-country" data-country="{{country_code}}">
		<img src="images/flags/{{country_code}}.png" class="icon" />
		<span class="country-name">{{name}}</span>
	</a>
</li>